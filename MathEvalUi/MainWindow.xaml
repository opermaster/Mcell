<Window x:Class="MathEvalUi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MathEvalUi"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="Cell_Grid" AutoGenerateColumns="True">
            <DataGrid.Columns>
                <DataGridTemplateColumn Header="1">
                    <DataGridTemplateColumn.CellTemplate>
                        <DataTemplate>
                            <TextBox BorderThickness="0" />
                        </DataTemplate>
                    </DataGridTemplateColumn.CellTemplate>
                </DataGridTemplateColumn>
                <DataGridTextColumn Header="2" Binding="{Binding b}"/>
                <DataGridTextColumn Header="3" Binding="{Binding c}"/>
                <DataGridTextColumn Header="4" Binding="{Binding d}"/>
                <DataGridTextColumn Header="5" Binding="{Binding e}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
